#include<bits/stdc++.h>
using namespace std;

int main(){
    int size;
    cin>>size;
    int arr[size];
    for(int i=0;i<size;i++){
        cin>>arr[i];
    }
    sort(arr,arr+size);
    for(int i=0;i<size;i++){
        cout<<"element"<<i+1<<" "<<arr[i]<<endl;
    }
    int n;
    n=arr[size-1];
    cout<<"largest no. "<<n;
    return 0;
}
