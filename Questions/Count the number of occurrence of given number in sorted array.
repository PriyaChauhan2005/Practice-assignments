#include<bits/stdc++.h>
using namespace std;

int main(){
    int size,number,t;
    t=0;
    cin>>size;
    int arr[size];
    for(int i=0;i<size;i++){
        cin>>arr[i];
    }
    sort(arr,arr+size);
    for(int i=0;i<size;i++){
        cout<<"sorted element"<<i+1<<" "<<arr[i]<<endl;
    }
    cout<<"Enter the no. to be checked:";
    cin>>number;
    for(int i=0;i<size;i++){
        if(arr[i]==number){
            t=t+1;
            }
        }
    cout<<"No. of occurence of given no."<<" "<<t;
    
}
